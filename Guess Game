import random

a = random.randint(0,10)

b = int(input(":"))
count = 1

while(b!=a):
    if b > a:
        print("choose smaller number")
    elif b < a :
        print("choose bigger number")
    b = int(input(":"))
    count +=1
   
if b == a :
    print("right guess")


print("number of tries = " , count)
